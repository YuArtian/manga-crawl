{
  "browser": {
    "headless": true,
    "maxInstances": 2,
    "timeout": 30000,
    "viewport": {
      "width": 390,
      "height": 844
    },
    "userAgent": "Mozilla/5.0 (iPhone; CPU iPhone OS 16_0 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/16.0 Mobile/15E148 Safari/604.1"
  },
  "direct": {
    "maxConcurrency": 8,
    "timeout": 15000,
    "retries": 3,
    "retryDelay": 1000
  },
  "download": {
    "outputDir": "./downloads",
    "stateDir": "./state"
  },
  "proxy": {
    "enabled": false,
    "url": ""
  },
  "log": {
    "level": "info"
  }
}