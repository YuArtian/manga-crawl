<script type="text/javascript">
    var sort = $(".sort");
    var type = 0;
    sort.click(function() {
        let sort_div = $(this).parent().parent().parent().find(".num_div");
        let sort_type = $(this).data('sort-type');
        if(sort_type !== "undefied") {
        	type = sort_type;
        }
        if(type == 0) {
            sortNum(sort_div, "desc");
            type = 1;
            $(this).data('sort-type', 1);
        } else {
            sortNum(sort_div, "asc");
            type = 0;
            $(this).data('sort-type', 0);
        }
    });

    var show_detail = $(".show_detail");
    var mask = $(".mask");
    var content_wrapper = $(".content-wrapper");
    var m = 0;
    show_detail.click(function() {
        if(m == 0) {
            content_wrapper.addClass("show");
            mask.hide();
            $(this).html("收起");
            m = 1;
        } else {
            content_wrapper.removeClass("show");
            mask.show();
            $(this).html("显示全文");
            m = 0;
        }
    });
    
